this.Today = function ()
{
	var sToday = "";
    var objDate = new Date();
    var sToday  = objDate.getFullYear() + "";
    var sMonth = objDate.getMonth()+1;
    var sDate = objDate.getDate();
    var sHours = objDate.getHours();
    var sMinutes = objDate.getMinutes();
    var sSeconds = objDate.getSeconds();
    
    if(sMonth.toString().length == 1){
		sMonth = "0" + sMonth;
    }
	if(sDate.toString().length == 1){
		sDate = "0" + sDate;
    }
    sToday = sToday+sMonth+sDate;
    return sToday;
}

this.No = function ()
{
	var sToday = "";
    var objDate = new Date();
    var sToday  = objDate.getFullYear() + "";
    var sMonth = objDate.getMonth()+1;
    var sDate = objDate.getDate();
    var sHours = objDate.getHours();
    var sMinutes = objDate.getMinutes();
    var sSeconds = objDate.getSeconds();
    
    if(sMonth.toString().length == 1){
		sMonth = "0" + sMonth;
    }
	if(sDate.toString().length == 1){
		sDate = "0" + sDate;
    }
    sNo = sToday+sMonth+sDate+sHours+sMinutes+sSeconds;
    return sNo;
}

/********************************************************************************
  처방 조회
********************************************************************************/
var gubun="";
this.wsInpaReceiptDiv_searchBtn_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{	
	var patNm = this.wsInpaReceiptDiv.patNmEd.value;				//환자 이름
	var diseaseNm = this.wsInpaReceiptDiv.diseaseNmEd.value;		//상병 명
	var inOut = this.wsInpaReceiptDiv.inOutCombo.value;				//입원&외래
	var prescGubun = this.wsInpaReceiptDiv.prescGubunCombo.value;	//처방 구분
	var startDay = this.wsInpaReceiptDiv.startDayCal.value;			//처방 시작
	var endDay = this.wsInpaReceiptDiv.endDayCal.value;				//처방 종료
	
	//patNo를 엊기 위해서 filter
	this.dsPat.filter("patNm == '"+patNm+"'");
	var patNo
	if(this.dsPat.rowcount > 0){
		for(var i=0;i<this.dsPat.rowcount;i++){
			patNo = this.dsPat.getColumn(i,"patNo");
		}
	}	
	
	var setPatNo;
	this.dsPat.filter("");
	this.dsPaientDiv.deleteAll();
	
	//alert("inOut : " + inOut);
	
	if(inOut.indexOf("입") != -1){
		setPatNo = "patNo='"+patNo+"' diseaseNm='"+diseaseNm+"' prescGubun='"+prescGubun+"' startDay='"+startDay+"' endDay='"+endDay+"' gubun='Y'";
		this.dsService.setColumn(0, "argument", setPatNo);
		this.gfnService("findSearchPatientPrscList", false);
		gubun="Y";
		//this.dsInPatient.copyData(this.dsPatientPrsc);
	}
	else if(inOut.indexOf("외") != -1){
		setPatNo = "patNo='"+patNo+"' diseaseNm='"+diseaseNm+"' prescGubun='"+prescGubun+"' startDay='"+startDay+"' endDay='"+endDay+"' gubun='N'";
		this.dsService.setColumn(0, "argument", setPatNo);
		this.gfnService("findSearchPatientPrscList", false);
		gubun="N";
		//this.dsOutPatient.copyData(this.dsPatientPrsc);
	}
	else{
		alert("입원/외래 구분을 선택해주세요!");
	}
}

