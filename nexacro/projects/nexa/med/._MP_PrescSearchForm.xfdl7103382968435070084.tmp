//환자 목록 클릭시
this.wsInpaReceiptDiv_whHosptlzReceiptGrid_oncellclick = function(obj:Grid, e:nexacro.GridClickEventInfo)
{
	this.prescTab.inTab.mpPrescDtlDiv.sdMedicationGrid.init();
	this.prescTab.outTab.mpPrescDtlDiv.sdMedicationGrid.init();
	
	var count = this.wsInpaReceiptDiv.whHosptlzReceiptGrid.currentrow;
	var prescNo;
	var medicationNo;
	if(gubun=="Y"){
		this.dsOutPatient.clearData();
		this.dsOutPrescDtl.clearData();
		this.dsOutMedication.clearData();
		
		this.dsInPatient.clearData();
		this.dsInPrescDtl.clearData();
		this.dsInMedication.clearData();
		
		this.dsInPatient.addRow();
		this.dsInPrescDtl.addRow();
		
		this.dsInPatient.copyRow(0,this.dsPatientPrsc, count);
		prescNo = this.dsInPatient.getColumn(0,"prescNo");
		medicationNo = this.dsInPatient.getColumn(0,"medicationNo");
	}
	else if(gubun=="N"){
		this.dsInPatient.clearData();
		this.dsInPrescDtl.clearData();
		this.dsInMedication.clearData();

		this.dsOutPatient.clearData();
		this.dsOutPrescDtl.clearData();
		this.dsOutMedication.clearData();
		
		this.dsOutPatient.addRow();
		this.dsOutPrescDtl.addRow();
		
		this.dsOutPatient.copyRow(0,this.dsPatientPrsc, count);	
		prescNo = this.dsOutPatient.getColumn(0,"prescNo");
		medicationNo = this.dsOutPatient.getColumn(0,"medicationNo");
	}
	
	var setPrescNo = "prescNo='"+prescNo+"'";
	var setMedicationNo = "medicationNo='"+medicationNo+"'";
	this.dsService.setColumn(5, "argument", setPrescNo);
	this.dsService.setColumn(4, "argument", setMedicationNo);
	
	alert("setMedicationNo : " + setMedicationNo);
	
	this.gfnService("findPrescDtl", false);
	this.gfnService("findSdMedicationList", false);
	
	if(gubun=="Y"){
		this.dsInPrescDtl.copyRow(0,this.dsPrescDtl, 0);
		this.dsInMedication.copyData(this.dsMedication);
	}
	else if(gubun=="N"){
		this.dsOutPrescDtl.copyRow(0,this.dsPrescDtl, 0);
		this.dsOutMedication.copyData(this.dsMedication);
	}
	
	strXml = this.dsMedication.saveXML("dsMedication");
	trace(strXml);
	
	//this.prescTab.set_enable(true);
}

