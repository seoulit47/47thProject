		this.cfrnTrmtRespBtn.set_visible(false);
	}else{
		alert("되는가?");
		this.cfrnTrmtRespBtn.set_visible(true);
    }
    
	var yearMonth=this.gfnDate("yyyymm");
	var year=yearMonth.substr(0,4);
