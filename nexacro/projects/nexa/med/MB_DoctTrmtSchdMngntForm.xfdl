	this.doctschdDiv.Div01.stYear.set_text(year);
	this.doctschdDiv.Div01.stMonth.set_text(month);
	this.dsService.setColumn(5,"argument","yearMonth="+yearMonth);
	
	this.gfnService("findCalendarList");
}

