					  this.dsJour.setColumn(creRow,"acntCd",array[5]);					//대변 계정코드셋팅
					  this.dsJour.setColumn(creRow,"acntNm",array[6]);					//대변 계정과목이름셋팅
					  this.dsJour.setColumn(creRow,"assiSubCd",array[7]);				//대변 보조과목코드셋팅
