        <Grid id="accAcntGrid" taborder="5" binddataset="dsAccAcnt" useinputpanel="false" autofittype="col" left="1.6%" top="88" right="3.21%" height="487" class="AreaGrid" oncelldblclick="accAcntGrid_oncelldblclick">
