		this.dsAppoint.setColumn(addAppmt,"empNm",empNm);
		this.dsAppoint.setColumn(addAppmt,"deptNm",deptnm);
		this.dsAppoint.setColumn(addAppmt,"deptCd",deptcd);
		this.dsAppoint.setColumn(addAppmt,"appmtyn","N");
						
