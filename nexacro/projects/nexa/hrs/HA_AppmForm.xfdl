		var deptcd=this.dsHmEmp.getColumn(this.dsHmEmp.rowposition,"deptCd");
		alert("deptnm: " + deptnm);
		alert("deptcd: " + deptcd);				
		this.dsAppoint.setColumn(addAppmt,"empNo",empNo);
